<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manyquiz.computers.full"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="13" />

    <application
        android:name="com.manyquiz.application.FullQuizApplication"
        android:allowBackup="false"
        android:icon="@drawable/launcher_main"
        android:label="@string/app_name"
        android:theme="@style/Theme.Light.NoTitleBar" >
        <activity
            android:name="com.manyquiz.activity.IntroActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.manyquiz.activity.QuizActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.manyquiz.activity.QuestionSuggestionActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.manyquiz.ReportFaultActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.manyquiz.InfoActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.manyquiz.activity.SettingsActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.manyquiz.activity.ResultsActivity"
            android:label="@string/title_activity_main" >
        </activity>
    </application>

</manifest>
